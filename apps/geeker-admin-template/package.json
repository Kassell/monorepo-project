    "qs": "^6.11.2",
    "screenfull": "^6.0.2",
    "sortablejs": "^1.15.2",
    "vue": "^3.3.21",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@limuen/viteconfig": "workspace:*",
    "@types/md5": "^2.3.5",
    "@types/nprogress": "^0.2.3",
